#ErrorDocument 404 http://www.servicebro.co.za/index.php


# Turn rewrite engine on
Options +FollowSymlinks
RewriteEngine on

# map neat URL to internal URL
RewriteRule ^getPath/(.+)/$   /foodly/src/AppBundle/Controller/GumtreeController.php?getPath=$1 [nc,qsa]
RewriteRule ^getConfig/(.+)/$   /foodly/src/AppBundle/Controller/GumtreeController.php?getConfig=$1 [nc,qsa]
RewriteRule ^getAdvertsByAccount/(.+)/$   /foodly/src/AppBundle/Controller/GumtreeController.php?getAdvertsByAccount=$1 [nc,qsa]
RewriteRule ^getAccountByEmail/(.+)/$   /foodly/src/AppBundle/Controller/GumtreeController.php?getAccountByEmail=$1 [nc,qsa]
RewriteRule ^updateAdvertState/(.+)/(.+)/$   /foodly/src/AppBundle/Controller/GumtreeController.php?updateAdvertState=$1&newstate=$2 [nc,qsa]



